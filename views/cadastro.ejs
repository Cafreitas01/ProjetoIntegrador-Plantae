<!-- Redes Sociais-->
<h3 class=" m-5 text-center text-success">Entre com</h3>
<section class=" mx-auto text-center text-success " style="width: 800px;">
  <button type="button" class="btn btn-primary btn-lg">Facebook</button>
<button type="button" class="btn btn-secondary btn-lg">Google</button>
</section>
<!-- Formulario-->
<section class="mx-auto m-5 text-success " style="width: 800px;">
<form>
  <div class="form-group">
    <label for="exampleInputName1">Nome </label>
    <input type="name" class="form-control form-control-sm" id="exampleInputName1" aria-describedby="nameHelp" placeholder="Nome Completo">
    <small id="nameHelp" class="form-text text-muted"></small>
  </div>
  <div class="form-group">
    <label for="exampleInputEmail1">Email </label>
    <input type="email" class="form-control form-control-sm" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Entre com seu E-mail">
    <small id="emailHelp" class="form-text text-muted"></small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Senha</label>
    <input type="password" class="form-control form-control-sm" id="exampleInputPassword1" placeholder="Senha">
  </div>
  <div class="form-group">
    <label for="exampleInputdataDeNascimento1">Data de Nascimento</label>
    <input type="DatadeNascimento" class="form-control form-control-sm" id="exampleInputDataDeNascimento1" aria-describedby="DataDeNascimentoHelp" placeholder="dd/mm/aaaa">
    <small id="DataDeNascimentoHelp" class="form-text text-muted"></small>
  </div>
  <div class="form-group">
    <label for="exampleInputCPF1">CPF</label>
    <input type="CPF" class="form-control form-control-sm" id="exampleInputCPF1" aria-describedby="CPFHelp" placeholder="___-___-___-__">
    <small id="CPFHelp" class="form-text text-muted"></small>
  </div>
  <div class="form-group">
    <label for="exampleInputTelefone1">Telefone</label>
    <input type="Telefone" class="form-control form-control-sm" id="exampleInputTelefone1" aria-describedby="TelefoneHelp" placeholder="Telefone">
    <small id="TelefoneHelp" class="form-text text-muted"></small>
  </div>
  <div class="form-check">
    <input type="checkbox" class="form-check-input" id="exampleCheck1">
    <label class="form-check-label" for="exampleCheck1">Sou Humano</label>
  </div>
  <div class="form-check">
    <input class="form-check-input" type="checkbox" value="" id="defaultCheck1">
    <label class="form-check-label" for="defaultCheck1">
      Li e Concordo com a <a href="./politicaDePrivacidade">Politica de Privacidade</a>
    </label>
    <br>

  <nav class="m-5">
    <div class="d-flex flex-row-reverse">
      <a class="btn btn-success p-2" href="./entrega" role="button">Cadastrar</a>
      </div>
    </button>
</form>

<br>
  </nav>
</section>

